{
    "name": "vue-docs",
    "displayName": "Vue docs",
    "description": "Просмотр документации [ Vue.js, Vuex, Vue Router, Vue SSR, Vuetify ] прямо из VS Code!",
    "version": "0.1.0",
    "publisher": "titiaiev",
    "engines": {
        "vscode": "^1.25.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:extension.openVueDocs"
    ],
    "main": "./extension",
    "contributes": {
        "commands": [
            {
                "command": "extension.openVueDocs",
                "title": "Open Vue Docs"
            }
        ]
    },
    "scripts": {
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "vscode": "^1.1.6"
    }
}
